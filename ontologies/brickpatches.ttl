brick:Equipment rdfs:subClassOf rec:Asset ;
    sh:property [ a sh:PropertyShape ;
            sh:name "feeds" ;
            sh:nodeKind sh:IRI ;
            sh:path rec:feeds ],
        [ a sh:PropertyShape ;
            sh:name "is fed by" ;
            sh:nodeKind sh:IRI ;
            sh:path rec:isFedBy ] .

